{
  "name": "Kurt",
  "scopeName": "source.kurt",
  "patterns": [
    {
      "name": "comment.line.semicolon.kurt",
      "match": ";.*$"
    },
    {
      "name": "string.quoted.double.kurt",
      "match": "\".*?\""
    },
    {
      "name": "keyword.control.first.kurt",
      "match": "\\b(?:var|const|infix|postfix|prefix|brackets|arity|bindop|chain|flat|sym|bool|alias)\\b"
    },
    {
      "name": "keyword.control.second.kurt",
      "match": "\\b(?:load|use|assume|case|show|def|let|pick|proof|qed|done|with|parse|tokenize|format|level|mode|context|trail|syntax|theory|help|break)\\b"
    },
    {
      "name": "keyword.control.third.kurt",
      "match": "\\b(?:contradiction|true|false)\\b"
    },
    {
      "name": "string.quoted.double.kurt",
      "match": "\\b(?:todo)\\b"
    }
  ]
}